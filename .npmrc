# npm 镜像配置
registry=https://registry.npmjs.org/

# 网络超时和重试配置
timeout=600000
fetch-retries=5
fetch-retry-mintimeout=20000
fetch-retry-maxtimeout=120000

# ARM64 构建配置
arch=arm64
target=28.0.0
disturl=https://electronjs.org/headers
runtime=electron
build_from_source=true

# 其他优化配置
audit=false
fund=false
